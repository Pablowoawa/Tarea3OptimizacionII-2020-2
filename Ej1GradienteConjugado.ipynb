    "from scipy.optimize import minimize\n",
    "\n",
    "f1 = lambda th: 2*(th[0]+1)**2 + 8*(th[1]+3)**2 +5*th[0] + th[1]\n",
    "epsilon = 0.001\n",
    "\n",
    "conj = minimize(f1,(10,10),method='CG',options={\"return_all\": True, \"gtol\":epsilon})\n",
    "\n",
    "conj.fun"
   ]
