   "source": [
    "from scipy.optimize import minimize\n",
    "\n",
    "f1 = lambda th: 100*((th[1]-th[0]**2)**2) + (1-th[0])**2\n",
    "\n",
    "conj = minimize(f1,(-1.5,-1),method='CG',options={\"return_all\": True, 'maxiter':200})\n",
    "\n",
    "conj.fun"
   ]
