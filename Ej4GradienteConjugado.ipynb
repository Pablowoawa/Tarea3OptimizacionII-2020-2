   "source": [
    "from scipy.optimize import minimize\n",
    "\n",
    "f1 = lambda th: 7*th[0]**2 + 2*th[0]*th[1] + th[1]**2 + th[0]**4 + th[1]**4\n",
    "\n",
    "conj = minimize(f1,(3,3),method='CG',options={\"return_all\": True, \"maxiter\":200})\n",
    "\n",
    "conj.fun\n"
   ]
